# Return value: Discarded
function check_setup()
{
	$ECHO "echo hbtn" > $TMP_FILE

	return 0
}

################################################################################
# The function 'sh_setup' will be called AFTER the execution of the students
# shell, and BEFORE the execution of the real shell (sh)
